{
  "JwtIssuer": "http://openimis.org",
  "JwtAudience": "http://openimis.org",
  "JwtExpireDays": 5,
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Language": {
    "Primary": "english,en,primary,0,1",
    "Languages": ["en", "fr"]
  },
  "AppPrefixDir": "",
  "AppSettings": {
    "FromPhone_Renewal": "/app/FromPhone/Renewal",
    "FromPhone_Renewal_Rejected": "/app/FromPhone/Renewal/Rejected",
    "FromPhone_Claim": "/app/FromPhone/Claim",
    "FromPhone_Claim_Rejected": "/app/FromPhone/Claim/Rejected",
    "FromPhone_Feedback": "/app/FromPhone/Feedback",
    "FromPhone_Feedback_Rejected": "/app/FromPhone/Feedback/Rejected",
    "Extracts_Phone": "/app/FromPhone/Extracts/Phone",
    "Enrollment_Phone": "/app/FromPhone/Enrollment",
    "Extracts_Offline": "/app/FromPhone/Extracts/Offline",
    "JsonDebugFolder": "/app/FromPhone/Enrollment/Debug",
    "SubmittedFolder": "/app/photos/Images/Submitted",
    "UpdatedFolder": "/app/photos/Images/Updated",
    "SmsStrings": "/app/Escape/Sms/Strings",
    "SmsStringsSecondary": "/app/Escape/Sms/StringsSecondaryLanguage"
  },
  "SmsGateWay": null,
  "Defaults": {
    "Family": {
      "Adults": 3,
      "Children": 2,
      "OtherAdults": 0,
      "OtherChildren": 0
    },
    "PriorEnrolment": false,
    "PeriodPayNotMatchedSms": 1
  },
  "MatchPaymentSchedule": {
    "JobName": "Match Payment Job",
    "CronExpression": "0 0 9 ? * MON-FRI"
  }
}
